generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Directions {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  description String
  professors String[]
  examplelink String
  additionallink String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
